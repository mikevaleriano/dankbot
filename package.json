{
  "name": "starter-node",
  "description": "Opinionated nodejs project starter - TS version.",
  "author": "Mike Valeriano <mkvlrn@gmail.com>",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "start": "node build/index",
    "prestart": "yarn build",
    "dev": "nodemon --exec babel-node src/index",
    "build": "rm -rf build && babel src --out-dir build",
    "lint": "eslint src/**/*.ts --fix"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "core-js": "^3.2.1",
    "discord.js": "^11.5.1",
    "regenerator-runtime": "^0.13.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "eslint-config-mikevaleriano": "^3.2.0",
    "nodemon": "^1.19.1",
    "typescript": "^3.6.2"
  }
}
